          <owl:Class rdf:about="#Review"/>
          <owl:Class rdf:about="#Description"/>
          <owl:Class rdf:about="#Metric"/>
          <owl:Class rdf:about="#Attachment"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://purl.org/gi2mo/ns</rdfs:isDefinedBy>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasAccessRight">
    <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://purl.org/gi2mo/ns</rdfs:isDefinedBy>
