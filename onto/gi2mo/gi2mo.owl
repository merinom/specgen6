    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="isIdeaOf"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="isDescriptionOf">
    <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://purl.org/gi2mo/ns</rdfs:isDefinedBy>
    <rdfs:domain rdf:resource="#Description"/>
