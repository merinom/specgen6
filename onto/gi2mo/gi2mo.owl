    <rdfs:comment xml:lang="en">Describes relationship between categories in Idea Management System, where one is classified as a part of another.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasAccessContol">
    <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
