    <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://purl.org/gi2mo/ns</rdfs:isDefinedBy>
    <rdfs:comment xml:lang="en">Indicates an Idea Management System in which the idea has been posted.</rdfs:comment>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#describesPartOf">
    <owl:inverseOf rdf:resource="#isDescribedIn"/>
