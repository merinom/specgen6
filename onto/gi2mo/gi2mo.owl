    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://www.holygoat.co.uk/owl/redwood/0.1/tags/taggedResource"/>
    </owl:inverseOf>
    <rdfs:label xml:lang="en">hasTag</rdfs:label>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasIdea">
    <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://purl.org/gi2mo/ns</rdfs:isDefinedBy>
    <rdfs:domain rdf:resource="#IdeaManagementSystem"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >hasIdea</rdfs:label>
    <rdfs:range rdf:resource="#Idea"/>
