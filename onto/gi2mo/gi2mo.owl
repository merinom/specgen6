    <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >http://www.w3.org/2004/02/skos/core</rdfs:isDefinedBy>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasCategory">
    <rdfs:comment xml:lang="en">Property indicating the category of idea or a review.</rdfs:comment>
